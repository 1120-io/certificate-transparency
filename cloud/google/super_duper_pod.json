{
  "id": "super-duper-node",
  "kind": "Pod",
  "apiVersion": "v1beta1",
  "desiredState": {
    "manifest": {
      "version": "v1beta1",
      "containers": [{
        "name": "registry",
        "image": "google/docker-registry",
        "ports": [{
          "name": "registry",
          "containerPort": 5000,
          "hostPort": 5000
        }],
        "env": [{
          "name": "GCS_BUCKET",
          "value": "ctlog_images"
        }]
      }, {
        "name": "super-duper",
        "image": "localhost:5000/alcutter/super_duper:test",
        "ports": [{
          "name": "super-duper",
          "containerPort": 6962,
          "hostPort": 6962
        }],
        "volumeMounts": [{
          "name": "log-data",
          "mountPath": "/mnt/ctlog"
        }]
      }, {
        "name": "etcd",
        "image": "graemej/k8s-etcd",
        "ports": [{
          "name": "client",
          "containerPort": 4001,
          "hostPort": 4001
        }, {
          "name": "peer",
          "containerPort": 7001,
          "hostPort": 7001
        }]
      }],
      "volumes": [{
        "name": "log-data"
       }]
     }
  },
  "labels": {"name": "log-node"}
}


